<% include ./partials/head %>
<script src='/test/views/js/gk3global.js' type='text/javascript'></script>
<script src='/test/views/js/drillHourlyFraud.js' type='text/javascript'></script>

<div class="modal-header">
    <h4 class="modal-title">Query Detail</h4>
</div>
<div class="modal-body">
	<input type="text" id="data" value="<%=data%>"  hidden>
	<input type="text" id="start" value="<%=start%>"  hidden>
	<input type="text" id="end" value="<%=end%>"  hidden>
	<div class="panel panel-default">
		<div class="panel-body">
			<form class="form-inline">
				<div class="form-group col-md-5">
					<label for="startDate">Date Range>=: </label>
					<input id="startDate" type="text" class="form-control" value="<%=start%>" placeholder="yyyy-MM-dd HH">
				</div>
				<div class="form-group col-md-5">
					<label for="endDate">Date Range<=: </label>
					<input id="endDate"  type="text" class="form-control" value="<%=end%>" placeholder="yyyy-MM-dd HH">
				</div>
				<div class="form-group col-md-2">
					<input type="button" class="btn btn-primary btn-sm" id="query" value="GO">
				</div>
			</form>
		</div>
	</div>
	<div class="panel panel-default">
		<div class="panel-body">
			<table class="table table-striped cell-border" width="100%" id="hourlyfraud-datatable">
				<thead>
					<tr>
						<th>Traffic Date</th>
						<th>Traffic Hour</th>
						<th>MSISDN</th>
						<th>IMSI</th>
						<th>Cell ID</th>
						<th>LAC</th>
						<th>IMEI</th>
						<th>Vendor</th>
						<th>Model</th>
						<th>Illegal Odds</th>
						<th>Illegal Odds Ad</th>
						<th>Illegal Odds7 Ad</th>
						<th>Calls Out</th>
						<th>B Count</th>
						<th>Num Out Adjuested</th>
						<th>B Ratio</th>
						<th>Num Out Adjuested Ratio </th>
						<th>Avg Dur</th>
						<th>Off Peak</th>
						<th>Bursting</th>
						<th>Cells</th>

						<th>Calls In</th>
						<th>SMS Out</th>
						<th>SMS Out IG</th>
						<th>SMS In</th>
						<th>Audit</th>
						<th>Seconds</th>
						<th>Units</th>
						<th>Possible Loss</th>
						<th>In Seconds</th>
						<th>In Units</th>
						<th>Avg In Units</th>
						<th>Identified Date</th>
						<th>Trunk In</th>
						<th>Trunk Out</th>

						<th>Domestic Out</th>
						<th>International Out</th>
					</tr>
				</thead>
			</table>
		</div>
	</div>
</div>
<footer> <% include ./partials/footer %> </footer>