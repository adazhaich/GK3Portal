<!DOCTYPE HTML>
<html>
<head>
<script src='/tunisiana/views/js/subscriberSummary.js' type='text/javascript'></script>

</head>
<body>
	<div class="container-fluid">
		<div id="page" class="container-fluid panel-group" style="margin-top: 20px; margin-left: 5px;">
			<div class="panel-group">
				<div class="panel panel-default">
					<div class="panel-heading">
						<h4 class="panel-title">
							<a role="button" data-toggle="collapse" href="#ssfilter_collapse"><i class="fa fa-angle-double-down"></i>Filter</a>
						</h4>
					</div>

					<div id="ssfilter_collapse" class="panel-collapse collapse">
						<div class="panel-body">
							<form class="form-inline">
								<div class="row" style="margin-left:0;">
									<div class="form-group col-md-4">
										<label for="subscriberName" class="col-sm-5 control-label">Subscriber Name: </label> 
										<div class="input-group col-sm-6 " >
										    <input type="text" id="subscriberName" class="form-control" value="">
										</div> 
									</div>
									<div class="form-group col-md-4">
										<label for="detections" class="col-sm-5 control-label">Detections: </label>
										<div class="input-group col-sm-2">
										    <input type="text" class="form-control" id="min" >
										</div> 
										<div class="input-group col-sm-1">
											~
										</div>
										<div class="input-group col-sm-2">
										    <input type="text" class="form-control" id="max" >
										</div> 
									</div>
									<div class="form-group col-md-1">
										&nbsp;
									</div>
									<div class="form-group col-md-3">
										&nbsp;
									</div>
								</div>
								
								<div class="row" style="margin-left:0;">
									<div class="form-group col-md-4">
										<label for="start" class="col-sm-5 control-label" >Start: </label> 
										<div class="input-group input-group-sm col-sm-6 date" data-provide="datepicker" data-date-format="yyyy-mm-dd">
										    <input type="text" id="start" class="form-control datepicker " value="">
										    <div class="input-group-addon">
										        <span class="glyphicon glyphicon-th"></span>
										    </div>
										</div> 
									</div>
									<div class="form-group col-md-4">
										<label for="end" class="col-sm-5 control-label">End: </label>
										<div class="input-group input-group-sm col-sm-6 date" data-provide="datepicker" data-date-format="yyyy-mm-dd">
										    <input type="text" id="end" class="form-control datepicker " value="">
										    <div class="input-group-addon">
										        <span class="glyphicon glyphicon-th"></span>
										    </div>
										</div> 
									</div>
									<div class="form-group col-md-1">
										&nbsp;
									</div>
									<div class="form-group col-md-3">
										<input type="button" class="btn btn-primary btn-sm" id="refresh" value="GO">
									</div>
								</div>
							</form>
						</div>
					</div>


				</div>
				
				<div class="panel panel-default">
					<div class="panel-heading">
						<h4 class="panel-title">
							<a role="button" data-toggle="collapse" href="#sstable_collapse"><i class="fa fa-angle-double-down"></i>Subscriber Summary
								 Report</a>
						</h4>
						<ul class="list-inline panel-actions">
							<li><a href="#" id="panel-fullscreen" data-table="subscribersummary-datatable" role="button"
								title="Toggle fullscreen" onclick="panleFull(this)"> <i
									class="glyphicon glyphicon-resize-full"></i>
							</a></li>
						</ul>
					</div>
					<div id="sstable_collapse" class="panel-collapse collapse in">
						<div class="panel-body">
									<table class="table table-striped cell-border" width="100%"
										id="subscribersummary-datatable">
										<thead>
											<tr>
												<th>Subscriber ID</th>
												<th>Subscriber Name</th>
												<th>Detections</th>	
												<th>Insert Time</th>
								
											</tr>
										</thead>
									</table>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</body>
</html>
