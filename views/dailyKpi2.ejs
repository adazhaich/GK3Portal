<!doctype html>
<html>
<head>
<script src='/test/views/js/gk3global.js' type='text/javascript'></script>
<script src='/test/views/js/dailyKpi2.js' type='text/javascript'></script>

</head>
<body>

	<div class="container-fluid">

		<div id="page" class="container-fluid panel-group"
			style="margin-top: 20px; margin-left: 5px;">
			<div class="panel-group">
				<div class="panel panel-default">
					<div class="panel-heading">
						<h10 class="panel-title">
							<a role="button" data-toggle="collapse" href="#<%=req.params.type%>filter_collapse"><i
								class="fa fa-angle-double-down"></i>Filter</a>
						</h10>
					</div>
					<div id="<%=req.params.type%>filter_collapse" class="panel-collapse collapse in">
						<div class="panel-body">
							<form class="form-inline" action="" method="get">
							<!-- 	store reprot type -->
								<input type="hidden" id="reportType" value="<%=req.params.type%>">
								<div class="row" style="margin-left: 0;">
									<div class="form-group col-md-4">
										<label for="start" class="col-sm-2">Start:</label>
										<div class="input-group input-group-sm date col-sm-6" data-provide="datepicker" data-date-format="yyyy-mm-dd">
											<input type="text" id="<%=req.params.type%>start" class="form-control datepicker">
											<div class="input-group-addon">
												<span class="glyphicon glyphicon-th"></span>
											</div>
										</div>
									</div>
									<div class="form-group  col-md-4">
										<label for="end" class="col-sm-2">End:</label>
										<div class="input-group input-group-sm date col-sm-6"
											data-provide="datepicker" data-date-format="yyyy-mm-dd">
											<input type="text" id="<%=req.params.type%>end" class="form-control datepicker">
											<div class="input-group-addon">
												<span class="glyphicon glyphicon-th"></span>
											</div>
										</div>
									</div>
									<div class="form-group col-md-1">&nbsp;</div>
									<div class="form-group col-md-1">
										<input type="button" value="Go" class="btn btn-primary btn-sm"
											id="<%=req.params.type%>refresh">
									</div>
								</div>
							</form>
						</div>
					</div>
				</div>
				<div class="panel panel-default">
					<div class="panel-heading">
					<h4 class="panel-title">
							<a role="button" id="graph_collapse_button" data-toggle="collapse"  href="#<%=req.params.type%>graph_collapse"><i
								class="fa fa-angle-double-down"></i>Graphs</a>
						</h4>
					</div>
					<div id="<%=req.params.type%>graph_collapse" class="panel-collapse collapse">
						<div class="panel-body">
							<div class="row" style="margin-left: 0;">
								<div  class="col-sm-6 col-md-4">
										<div  class="caption" >
											<strong>New Detections</strong>
										</div>
								
									<div class="thumbnail zoom" id="<%=req.params.type%>newDetections-chart">
									</div>
								</div>
								
								<div class="col-sm-6 col-md-4">
										<div class="caption">
											<strong>Fraud Activity</strong>
										</div>
									<div class="thumbnail zoom" id="<%=req.params.type%>fraudActivity-chart">

									</div>
								</div>
								<div class="col-sm-6 col-md-4">
										<div class="caption">
											<strong>Median Loss</strong>
										</div>
									<div class="thumbnail zoom" id="<%=req.params.type%>medianLoss-chart">
									</div>
								</div>
								</div>
								<div  class="row" style="margin-left: 0;">
									<div class="col-sm-6 col-md-4">
										<div class="caption">
											<strong>New Detections</strong>
										</div>
									
									<div class="thumbnail zoom" id="<%=req.params.type%>newDetections-pieChart">
									</div>
								</div>
								<div class="col-sm-6 col-md-4">
										<div class="caption">
											<strong>International Revenue</strong>
										</div>
									<div class="thumbnail zoom" id="<%=req.params.type%>iRevenue-chart">
									</div>
								</div>
								<div class="col-sm-6 col-md-4">
										<div class="caption">
											<strong>Median Loss</strong>
										</div>
									<div class="thumbnail zoom" id="<%=req.params.type%>medianLoss-pieChart">
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
			<div class="panel panel-default">
				<div class="panel-heading">
					<h4 class="panel-title">
						<a role="button" id="table_title" data-toggle="collapse"
							href="#<%=req.params.type%>table_collapse"><i class="fa fa-angle-double-down"></i>Daily	KPI 
							<% 
								var type = req.params.type;
								if ("onnet" == type) { %>
									On-net
						    <% }
						    else if ("corporate" == type) {%>
									Corporate
						   <%} 			
						   else if ("offnet" == type) {%>
									Off-net  
						   <%} 			
					    	%>					
						</a>
					</h4>


					<ul class="list-inline panel-actions">

					Total Records:	<li class="active"><a align='left' id="record_count"></a></li>
						<li>
							<a href="#" id="panel-fullscreen" data-table="<%=req.params.type%>dailyKpi-datatable2" role="button" title="Toggle fullscreen" onclick="panleFull(this)"><i class="glyphicon glyphicon-resize-full"></i></a>
						</li>
					</ul>

				</div>
				<div id="<%=req.params.type%>table_collapse" class="panel-collapse collapse">
					<div class="panel-body">
						<table class="table table-striped cell-border"
							id="<%=req.params.type%>dailyKpi-datatable2">
							<thead>
								<tr>
									<th rowspan="3">Date</th>
									<th colspan="7" style="background-color: lightyellow;">New Detections (Unique)</th>
									<th colspan="5">Fraud Activity</th>
									<th colspan="6" style="background-color: lightyellow;">Median MOU Before</th>
									<th colspan="6" style="background-color: lightyellow;">Median MOU After</th>
									<th colspan="4">Median Loss</th>
									<th colspan="4" style="background-color: lightyellow;">False Positive</th>
								</tr>
								<tr>
									<th colspan="3"  style="background-color: lightblue;">Mobius</th>
									<th colspan="4"  style="background-color: lightgreen;">Other</th>
									<th rowspan="2">SIMs*</th>
									<th rowspan="2" >MOU Before</th>
									<th rowspan="2">MOU After</th>
									<th rowspan="2">Total MOU</th>
									<th rowspan="2">Total Loss</th>

									<th colspan="3" style="background-color: lightblue;">Mobius</th>
									<th colspan="2"  style="background-color: lightgreen;">Other</th>

									<th rowspan="2">All</th>
									<th colspan="3" style="background-color: lightblue;">Mobius</th>
									<th colspan="2" style="background-color: lightgreen;">Other</th>

									<th rowspan="2">All</th>

									<th rowspan="2">Before</th>
									<th rowspan="2">After</th>
									<th rowspan="2">Total</th>
									<th rowspan="2">Remaining Balance</th>


									<th colspan="2" style="background-color: lightblue;">Mobius GK</th>
									<th colspan="2" style="background-color: lightgreen;">Mobius GK - C</th>

								</tr>
								<tr>
									<th>GK</th>
									<th>GK-C</th>
									<th>TCG</th>
									<th>TCG</th>
									<th>FMS</th>
									<th>Total</th>
									<th>Total Balance Remaining</th>

									<th>GK</th>
									<th>GK-C</th>
									<th>TCG</th>
									<th>TCG</th>
									<th>FMS</th>
									<th>GK</th>
									<th>GK-C</th>
									<th>TCG</th>
									<th>TCG</th>
									<th>FMS</th>
									<th>Count</th>
									<th>%</th>
									<th>Count</th>
									<th>%</th>
								</tr>
							</thead>
						</table>
					</div>
				</div>
			</div>
		</div>
	</div>
</body>
</html>
