<!DOCTYPE HTML>
<html>
<head>
<script src='/test/views/js/dailyKpi.js' type='text/javascript'></script>
</head>
<body>
	<div class="container-fluid">
		<div id="page" class="container-fluid panel-group"
			style="margin-top: 20px; margin-left: 5px;">
			<div class="panel-group">
				<div class="panel panel-default">
					<div class="panel-heading">
						<h4 class="panel-title">
							<a role="button" data-toggle="collapse" href="#filter_collapse"><i
								class="fa fa-angle-double-down"></i>Filter</a>
						</h4>
					</div>
					<div id="filter_collapse" class="panel-collapse collapse">
						<div class="panel-body">
								<div class="row" style="margin-left:0;"></div>
								<div class="row" style="margin-left: 0;">
									<div class="form-group col-md-3">
										<label for="day" class="col-sm-3">Date:</label> 
										<div class="input-group col-sm-8 date" data-provide="datepicker" data-date-format="yyyy-mm-dd">
										    <input type="text" id="day" class="form-control datepicker " value="">
										    <div class="input-group-addon">
										        <span class="glyphicon glyphicon-th"></span>
										    </div>
										</div> 
									</div>
									<div class="form-group col-md-4">
										&nbsp;
									</div>
								</div>
								<div class="row" style="margin-left: 0;">
									<div class="form-group col-md-3">
										<label for="start" class="col-sm-3">Start : </label> 
										<div class="input-group col-sm-8 date" data-provide="datepicker" data-date-format="yyyy-mm-dd">
										    <input type="text" id="start" class="form-control datepicker " value="">
										    <div class="input-group-addon">
										        <span class="glyphicon glyphicon-th"></span>
										    </div>
										</div> 
									</div>
									<div class="form-group col-md-3">
										<label for="end" class="col-sm-3">End : </label>
										<div class="input-group col-sm-8 date" data-provide="datepicker" data-date-format="yyyy-mm-dd">
										    <input type="text" id="end" class="form-control datepicker " value="">
										    <div class="input-group-addon">
										        <span class="glyphicon glyphicon-th"></span>
										    </div>
										</div> 
									</div>
									<div class="form-group col-md-1">
										<button class="btn btn-primary" id="refresh">Go</button>
									</div>
								</div>
						</div>
					</div>
				</div>
				
				<div class="panel panel-default">
					<div class="panel-heading">
						<h4 class="panel-title">
							<a role="button" id="table_title" data-toggle="collapse" href="#table_collapse"><i
								class="fa fa-angle-double-down"></i>Daily KPI Report</a>
						</h4>
					</div>
					<div id="table_collapse" class="panel-collapse collapse in">
						<div class="panel-body">
							<table class="table datatable table-hover" 
							id="dailyKpi-datatable">
							<thead>
								<tr>
									<th rowspan="3">Date</th>
									<th colspan="7">New Detections (Unique)</th>
									<th colspan="5">Fraud Activity</th>
									<th colspan="6">Median MOU Before</th>
									<th colspan="6">Median MOU After</th>
									<th colspan="4">Median Loss</th>
									<th colspan="4">False Positive</th>
								</tr>
								<tr>
									<th colspan="3">Mobius</th>
									<th colspan="4">Other</th>
									<!-- 										<th colspan="5"></th> Fraud Activity -->

									<th rowspan="2">SIMs*</th>
									<th rowspan="2">MOU Before</th>
									<th rowspan="2">MOU After</th>
									<th rowspan="2">Total MOU</th>
									<th rowspan="2">Total Loss</th>


									<th colspan="3">Mobius</th>
									<th colspan="2">Other</th>
									<!-- 										Median MOU Before -- All -->
									<th rowspan="2">All</th>
									<th colspan="3">Mobius</th>
									<th colspan="2">Other</th>
									<!-- 										Median MOU After -- All -->
									<th rowspan="2">All</th>
									<!-- 										Median Loss -->
									<th rowspan="2">Before</th>
									<th rowspan="2">After</th>
									<th rowspan="2">Total</th>
									<th rowspan="2">Remaining Balance</th>


									<th colspan="2">Mobius GK</th>
									<th colspan="2">Mobius GK - C</th>
									<!-- 										<th colspan="4">Median Loss</th>
										<th colspan="4">False Positive</th>		 -->
								</tr>
								<tr>
									<th>Mobius GK</th>
									<th>Mobius GK-C</th>
									<th>Mobius TCG</th>
									<th>TCG</th>
									<th>FMS</th>
									<th>Total</th>
									<th>Total Balance Remaining</th>
									<!-- 
										<th>SIMs*</th>
										<th>MOU Before</th>
										<th>MOU After</th>
										<th>Total MOU</th>
										<th>Total Loss</th>
 -->
									<th>GK</th>
									<th>GK-C</th>
									<th>TCG</th>
									<th>TCG</th>
									<th>FMS</th>
									<!-- 										<th rowspan="2">All</th> -->

									<th>GK</th>
									<th>GK-C</th>
									<th>TCG</th>
									<th>TCG</th>
									<th>FMS</th>
									<!-- 										<th rowspan="2">All</th> -->

									<!-- 										<th rowspan="2">Before</th>
										<th rowspan="2">After</th>
										<th rowspan="2">Total</th>
 										<th rowspan="2">Remaining Balance</th>
 -->
									<th>Count</th>
									<th>%</th>
									<th>Count</th>
									<th>%</th>
								</tr>
							</thead>
						</table>
						</div>
					</div>
				</div>
				</div>
			</div>
		</div>
		<footer><% include ./partials/footer %> </footer>
	</div>
</body>
</html>
