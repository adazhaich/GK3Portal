<!doctype html>
<html>
<head>
<script src='/test/views/js/dailyFraudOtt.js' type='text/javascript'></script>

</head>
<body>
	<div class="container-fluid">

		<div id="page" class="container-fluid panel-group"
			style="margin-top: 20px; margin-left: 5px;">
			<div class="panel-group">
				<div class="panel panel-default">
					<div class="panel-heading">
						<h4 class="panel-title">
							<a role="button" data-toggle="collapse" href="#dfofilter_collapse"><i
								class="fa fa-angle-double-down"></i>Filter</a>
						</h4>
					</div>
					<div id="dfofilter_collapse" class="panel-collapse collapse">
						<div class="panel-body">
							<form class="form-inline" action="" method="get">
								<div class="row" style="margin-left: 0;">
									<div class="form-group col-md-4">
										<label for="name" class="col-sm-2">Day:</label>
										<div class="input-group input-group-sm col-sm-6 date" data-provide="datepicker" data-date-format="yyyy-mm-dd">
											<input type="text" id="day" class="form-control datepicker ">
											<div class="input-group-addon">
												<span class="glyphicon glyphicon-th"></span>
											</div>
										</div>
									</div>
								</div>
								<div class="row" style="margin-left: 0;">
									<div class="form-group col-md-4">
										<label for="start" class="col-sm-2">Start:</label>
										<div class="input-group input-group-sm date col-sm-6" data-provide="datepicker" data-date-format="yyyy-mm-dd">
											<input type="text" id="start" class="form-control datepicker">
											<div class="input-group-addon">
												<span class="glyphicon glyphicon-th"></span>
											</div>
										</div>
									</div>
									<div class="form-group  col-md-4">
										<label for="end" class="col-sm-4">End:</label>
										<div class="input-group input-group-sm date col-sm-6"
											data-provide="datepicker" data-date-format="yyyy-mm-dd">
											<input type="text" id="end" class="form-control datepicker">
											<div class="input-group-addon">
												<span class="glyphicon glyphicon-th"></span>
											</div>
										</div>
									</div>
									<div class="form-group col-md-1">
										<input type="button" value="Go" class="btn btn-primary btn-sm"
											id="refresh">
									</div>
								</div>
							</form>
						</div>
					</div>
				</div>
			</div>
			<div class="panel panel-default">
				<div class="panel-heading">
					<h4 class="panel-title">
						<a role="button" id="table_title" data-toggle="collapse"
							href="#dfotable_collapse"><i class="fa fa-angle-double-down"></i>Daily	KPI TCG
						</a>
					</h4>
					<ul class="list-inline panel-actions">
						<li>
							<a href="#" id="panel-fullscreen" role="button" title="Toggle fullscreen" onclick="panleFull(this)"><i class="glyphicon glyphicon-resize-full"></i></a>
						</li>
					</ul>
				</div>
				<div id="dfotable_collapse" class="panel-collapse collapse in">
					<div class="panel-body">
						<table class="table table-striped cell-border"
							id="dailyFraudOtt-datatable">
							<thead>
								<tr>
									<th>Traffic Date</th>
									<th>Call Count</th>
									<th>Detections PCT</th>
									<th>Calling Service</th>
									<th>Detections Count</th>
									<th>Calling Country</th>
								</tr>
							</thead>
						</table>
					</div>
				</div>
			</div>
		</div>
	</div>
</body>
</html>