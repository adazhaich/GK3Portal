<!doctype html>
<html>
<head>

    <script src='/tunisiana/views/js/gk3global.js' type='text/javascript'></script>
    <script src='/tunisiana/views/js/detectionDetails.js' type='text/javascript'></script>
    <script src="/tunisiana/views/js/map.js"  type='text/javascript'></script>
</head>
<body>
<div class="container-fluid">
    <div id="page" class="container-fluid panel-group" style="margin-top: 20px; margin-left: 5px;">
        <div class="panel-group">
            <div class="panel panel-default">
                <div class="panel-heading" >
                    <h4 class="panel-title">
                        <a role="button" style="font-size: 10px; text-decoration: none" data-toggle="collapse" href="#ddfilter_collapse"><i
                                    class="fa fa-angle-double-down"></i>Filter</a>
                    </h4>
                </div>

                <div id="ddfilter_collapse" class="panel-collapse collapse in">
                    <div class="panel-body">
                  <!--      <div id="reportrange" class="pull-right" style="background: #fff; cursor: pointer; padding: 5px 10px; border: 1px solid #ccc; width: 100%">
                            <i class="glyphicon glyphicon-calendar fa fa-calendar"></i>&nbsp;
                            <span></span> <b class="caret"></b>
                        </div>

                        &lt;!&ndash; &ndash;&gt;-->
                            <div class="form-group col-md-2">
                                <!--<label for="start" class="col-sm-3">Start: </label>-->

                                <div class="input-group input-group-sm date form_datetime">
                               <input id="start" class="form-control" type="text" value="">
                                    <span class="input-group-addon"><span class="glyphicon glyphicon-th"></span></span>
                                </div>
                            </div>
                            <div class="form-group col-md-2">


                                <div class="input-group input-group-sm date form_datetime">

                                    <input id="end" class="form-control" type="text" value="">
                                    <span class="input-group-addon"><span class="glyphicon glyphicon-th"></span></span>
                                </div>
                            </div>
                            <div class="form-group col-sm-1">
                                <input type="button" class="btn btn-primary btn-sm" id="refresh" value="GO">
                            </div>
                            <div class="form-group col-sm-1">
                                <button class="btn btn-primary btn-sm" id="advanced" onclick="displayDiv()">Advanced
                                </button>
                                <input type="hidden" id="reportType" value="dd_div"/>
                            </div>

                        <% var report = 'dd' %>
                        <% include filterParam %>
                    </div>
                </div>


            </div>
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h4 class="panel-title">
                        <a role="button" data-toggle="collapse" href="#ddgraph_collapse"><i
                                    class="fa fa-angle-double-down"></i>Graphs</a>
                    </h4>
                </div>
                <div id="ddgraph_collapse" class="panel-collapse collapse in">
                    <div class="panel-body">
                        <div class="row" style="margin-left: 0;">
                            <div class="col-sm-6">
                                <div class="caption">
                                    <!--<strong>MOU By Cell Id</strong>-->
                                    Top 30 MOUs by Cell Id
                                </div>
                                <div class="thumbnail zoom" id="sumOutDurationByCell-chart">
                                </div>
                            </div>
<!--                            <div class="col-sm-6 col-md-4">
                                <div class="caption">
                                    <strong>Cell Info</strong>
                                </div>
                                <input type="text" id="mapId" value="<%= mapId %>" hidden>
                                <div id="<%= mapId %>" style="width: 100%; height: 225px"></div>
                                <div class="thumbnail zoom">
                                </div>
                            </div>-->
                            <div class="col-sm-6">

                                <div class="caption">
                                    Detections By Hour
                                </div>
                                <div class="thumbnail zoom" id="dateHour-chart">
                                </div>
                            </div>
                        </div>
                        <div class="row" style="margin-left: 0;">
                            <div class="col-sm-6">
                                <div class="caption">
                                    <!--<strong>Calls By Cell Id</strong>-->
                                    Top 30 Calls Count  By Cell Id
                                </div>
                                <div class="thumbnail zoom" id="callsOutByCellId-chart">
                                </div>
                            </div>
                            <!--<div class="col-sm-6 col-md-4"></div>-->
                            <div class="col-sm-6">
                                <div class="caption">
                                   Detections By Hour
                                </div>
                                <div class="thumbnail zoom" id="dateHour-piechart">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div id="test" class="panel panel-default">
                <div class="panel-heading">
                    <h4 class="panel-title">
                        <a role="button" data-toggle="collapse" href="#table_collapse"><i
                                    class="fa fa-angle-double-down"></i>Detection
                            Details Report</a>
                    </h4>
                    <ul class="list-inline panel-actions">
                        Total Records:
                        <li class="active"><strong align='left' id="dd_record_count"></strong></li>
                        <li>
                            <a href="#" id="panel-fullscreen" data-table="detectionDetails-datatable" role="button"
                               title="Toggle fullscreen" onclick="panleFull(this)">
                                <i class="glyphicon glyphicon-resize-full"></i>
                            </a>
                        </li>
                    </ul>
                </div>
                <div id="table_collapse" class="panel-collapse collapse in">
                    <div class="panel-body">

                        <table class="table table-striped cell-border"
                               id="detectionDetails-datatable">
                            <thead>
                            <tr style="background-color: lightseagreen;">
                                <th>Detection Time</th>
                                <th>Call Date Hour</th>
                                <th>Source</th>
                                <th>First</th>
                                <th>Operator</th>
                                <th>Corp</th>
                                <th>MSISDN</th>
                                <th>IMSI</th>
                                <th>IMEI</th>
                                <th>LAC</th>
                                <th>Cell ID</th>
                                <th>Vendor</th>
                                <th>Model</th>
                                <th>Calls</th>
                                <th>MOU</th>
                                <th>H/M</th>
                                <th>SubID</th>
                                <th>Dealer ID</th>
                                <th>Activation Date</th>
                                <th>Last Registration Date</th>
                                <th>Last TopUp Date</th>
                                <th>Last TopUp Amount</th>
                                <th>Remaining Balance</th>
                                <th>Data Usage</th>
                                <!--<th>Status</th>-->
                                <th>Shutdown File</th>
                                <!--<th>False Positive</th>-->
                            </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade modal-fullscreen force-fullscreen" id="detectionDetailModal" tabindex="-1" role="dialog"
     aria-hidden="true">
    <div class="modal-dialog" style="height:800px;">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                            aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Corporate Summary</h4>
            </div>
            <div class="modal-body">
                <form class="form-horizontal">
                    <div class="form-group">
                        <label for="corporateId" class="col-sm-3 control-label">Corporate Id</label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control" id="corporateId">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="corporateName" class="col-sm-3 control-label">Corporate Name</label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control" id="corporateName">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="msisdn" class="col-sm-3 control-label">MSISDN</label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control" id="msisdn">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="comment" class="col-sm-3 control-label">COMMENT</label>
                        <div class="col-sm-8">
                            <input type="text" class="form-control" id="comment">
                        </div>
                    </div>
                </form>
                <div id="message" class="alert alert-danger" hidden="true" role="alert">Add Or Update Failed</div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary" id="saveCorp">Save</button>
            </div>
        </div>
    </div>
</div>
</body>
</html>